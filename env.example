# Cribl Control Plane SDK Configuration
# Copy this file to .env and fill in your actual values

# Your Cribl Control Plane server URL
CRIBL_SERVER_URL=https://your-cribl-instance.com

# Your API bearer token for authentication
CRIBL_BEARER_TOKEN=your-api-token-here

# Optional: Enable debug logging for the SDK
CRIBLCONTROLPLANE_DEBUG=false
